{
  "name": "solc",
  "version": "0.6.1",
  "description": "Solidity compiler",
  "main": "index.js",
  "bin": {
    "solcjs": "solcjs"
  },
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint": "node ./node_modules/semistandard/bin/cmd.js",
    "prepublish": "node downloadCurrentVersion.js && node verifyVersion.js",
    "pretest": "npm run lint",
    "test": "tape ./test/index.js",
    "test:nyc": "nyc tape ./test/index.js | tap-nyc",
    "test:jest": "jest --verbose --detectOpenHandles",
    "coverage:jest": "jest --coverage",
    "coverage": "node ./node_modules/nyc/bin/nyc.js --reporter=lcov --reporter=text-summary ./node_modules/tape/bin/tape ./test/index.js",
    "coveralls": "npm run coverage && node ./node_modules/coveralls/bin/coveralls.js <coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethereum/solc-js.git"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "compiler"
  ],
  "files": [
    "abi.js",
    "index.js",
    "linker.js",
    "smtchecker.js",
    "smtsolver.js",
    "solcjs",
    "soljson.js",
    "translate.js",
    "wrapper.js"
  ],
  "author": "chriseth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ethereum/solc-js/issues"
  },
  "homepage": "https://github.com/ethereum/solc-js#readme",
  "dependencies": {
    "axios": "^0.19.1",
    "command-exists": "^1.2.8",
    "commander": "3.0.2",
    "fs-extra": "^0.30.0",
    "js-sha3": "0.8.0",
    "memorystream": "^0.3.1",
    "require-from-string": "^2.0.0",
    "semver": "^5.5.0",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "coveralls": "^3.0.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.5",
    "nyc": "^14.1.0",
    "semistandard": "^12.0.1",
    "tap-nyc": "^1.0.3",
    "tape": "^4.11.0",
    "tape-spawn": "^1.4.2",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.5"
  },
  "semistandard": {
    "ignore": [
      "soljson.js"
    ]
  },
  "nyc": {
    "exclude": [
      "soljson.js"
    ]
  }
}
